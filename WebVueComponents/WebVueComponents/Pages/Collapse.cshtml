       let app = new Vue({
           el: '#app',
           data() {
               return {
                   dataurl: '@Url.Content("/api/values/addressbookdata")',
                   option1: {
                       totalRecords: 0,
                       pagePerCount: 10,
                       noPageLinks: 3,
                       isDataLoaded: false,
                       collapseId: 'collapse_1'
                   },
                   option2: {
                       totalRecords: data.length,
                       pagePerCount: 5,
                       noPageLinks: 3,
                       isDataLoaded: false,
                       collapseId: 'collapse_2'
                   } 
               }
           },
           methods: {
               pageChanged: function (page) {
                   let vm = this; vm.collapseChild.showLoader = true;
                   vm.callApi({ page: page, length: vm.option1.pagePerCount });
               },
               callApi: function (dataParam) {
                   let vm = this;
                   $.ajax({
                       url: vm.dataurl,
                       method: 'GET',
                       data: Object.assign({}, dataParam),
                       success: function (r) {
                           vm.option1.totalRecords = r.total;
                           vm.collapseChild.collapseData = r.records;
                           vm.collapseChild.showLoader = false;
                           vm.option1.isDataLoaded = true;
                       }
                   });
               },
               pageChanged1: function (page) {
                   let vm = this; vm.collapseChild1.showLoader = true;
                   vm.refreshCollapseData({ page: page, length: vm.option2.pagePerCount });
               },
               refreshCollapseData: function (d) {
                   let vm = this; vm.option2.isDataLoaded = true;
                   vm.collapseChild1.collapseData = data.slice((d.page - 1) * d.length, (d.length * d.page));
                   vm.collapseChild1.showLoader = false;
               }
           },
           mounted: function () {
               let vm = this; 
               vm.collapseChild.showLoader = true;
               vm.callApi({ page: 1, length: vm.option1.pagePerCount });
               vm.collapseChild1.showLoader = true;
               vm.refreshCollapseData({ page: 1, length: vm.option2.pagePerCount });
           },
           computed: {
               collapseChild: function () {
                   let vm = this;
                   return vm.$children.find(q => q.$attrs.id == vm.option1.collapseId);
               },
               collapseChild1: function () {
                   let vm = this;
                   return vm.$children.find(q => q.$attrs.id == vm.option2.collapseId);
               }
           }
       });

    </script>
